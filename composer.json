{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "balbuf/drupal-libraries-installer": "^1.0",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/addtoany": "^1.12",
        "drupal/admin_toolbar": "^1.26",
        "drupal/backup_migrate": "4.0",
        "drupal/ckeditor_config": "^2.1",
        "drupal/coder": "^8.3",
        "drupal/components": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/context": "^4.0",
        "drupal/core": "^8.7.7",
        "drupal/custom_nid": "^1.0",
        "drupal/devel": "^1.2",
        "drupal/ds": "^3.3",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/easy_social": "^3.0",
        "drupal/entity_browser": "^2.1",
        "drupal/entity_class_formatter": "^1.1",
        "drupal/entity_embed": "^1.0",
        "drupal/field_formatter_class": "^1.1",
        "drupal/iframe": "^1.9",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/media_entity": "^1.8",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.4",
        "drupal/radix": "^4.1",
        "drupal/radix_layouts": "^4.1",
        "drupal/single_datetime": "^1.7",
        "drupal/sitemap": "^1.5",
        "drupal/token": "^1.5",
        "drupal/video_embed_field": "^2.1",
        "drupal/video_embed_html5": "^1.1",
        "drupal/views_bootstrap": "^4.1",
        "drupal/webform": "^5.2",
        "drush/drush": "^9.0.0",
        "egulias/email-validator": "^2.0",
        "psr/container": "^1.0",
        "squizlabs/php_codesniffer": "^3.4",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
