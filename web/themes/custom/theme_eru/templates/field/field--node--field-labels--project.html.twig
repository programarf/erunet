{#
/**
 * @file
 * Template for a field.
 */
#} 
{% if label_hidden %}
  {% if multiple %}
    <div{{ attributes }}>
      {% for item in items %}
        <div{{ item.attributes }}>{{ item.content }}</div>
      {% endfor %}
    </div>
  {% else %}
    {% for item in items %}
      <div{{ attributes }}>{{ item.content }}</div>
    {% endfor %}
  {% endif %}
{% else %}  
    <h3{{ title_attributes }}>{{ label }}</h3>
    {% if multiple %}
    <div>
      {% endif %}      
      {% for item in items %}
        <div class="tag_alcance"><span>{{ item.content }}</span></div>      
      {% endfor %}
      {% if multiple %}
    </div>
    {% endif %}  
{% endif %}